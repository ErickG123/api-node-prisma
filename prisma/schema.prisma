// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Product {
  id         String   @id @default(uuid())
  name       String
  value      Decimal
  created_at DateTime @default(now())

  @@map("products")
}
